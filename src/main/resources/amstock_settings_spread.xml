<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <type>bar</type>                     
  <data_type>csv</data_type>           
  
  <column>
    <width>95</width>                  
    <spacing>0</spacing>               
    <grow_time>6</grow_time>           
    <alpha>60</alpha>                  
    <data_labels>
      <![CDATA[{series}: {value} Litres]]> 
    </data_labels>
    <data_labels_text_color>#FFFFFF</data_labels_text_color> 
    <data_labels_position>inside</data_labels_position>      
                                                                                                                              
    <balloon_text>                                                    
      <![CDATA[{series}: <b>{value} Litres</b>]]>           
    </balloon_text>    
    <gradient>horizontal</gradient>
    <sequenced_grow>true</sequenced_grow>                         
  </column>

     
  <plot_area>                 
    <margins>                 
      <left>33</left>          
      <top>120</top>           
      <right>125</right>      
      <bottom>40</bottom>     
    </margins>
  </plot_area>
  
  <grid>                      
    <category>                                                                     
      <alpha>0</alpha>        
    </category>
    <value>                         
      <alpha>0</alpha>        
    </value>
  </grid>
  
  <values>                    
    <category>                
      <enabled>false</enabled>
    </category>
    <value>                   
      <enabled>false</enabled>
      <min>0</min>            
    </value>
  </values>
  
  <axes>                      
    <category>                
      <alpha>0</alpha>        
    </category>
    <value>                   
      <alpha>0</alpha>        
    </value>
  </axes>  
    
  <legend>                      
    <enabled>false</enabled>    
  </legend>  
  
  <labels>                      
    <label>
      <x>30</x>                                
      <y>80</y>                                
      <rotate></rotate>                        
      <text_color>#FFFFFF</text_color>         
      <text_size>13</text_size>                
      <text>                                   
        <![CDATA[<b>Beer consumption by country (litres)</b>]]>
      </text>        
    </label> 
  </labels>
  
  <balloon>                    
    <text_color>000000</text_color>
    <corner_radius>4</corner_radius>
    <border_width>1</border_width>
    <border_alpha>100</border_alpha>
    <border_color>#FFFFFF</border_color>
    <alpha>80</alpha>
    <color>#FFFFFF</color>
  </balloon>  
  
  <graphs>
    <graph>
      <gradient_fill_colors>#000000, #FF6600</gradient_fill_colors>
    </graph>
  </graphs>
</settings>
